      "cell_type": "code",
      "source": [
        "tau = -1\n",
        "y = x.subs(t, tau - t)\n",
        "x2 = x.subs(t, t-1)\n",
        "\n",
        "\n",
        "px=sym.plot(x2, (t, -7, 7), ylim=[-0.2, 1.5], ylabel=r'$y(t)$', line_color='gray', show=False)\n",
        "py=sym.plot(y, (t, -7, 7), ylim=[-0.2, 1.5], ylabel=r'$y(t)$', show=False)\n",
        "px.extend(py)\n",
